!-------------------------------------------------------------------------------
! general model setup
!-------------------------------------------------------------------------------
&glm_setup
   sim_name = 'FCR GLM Simulation'
   max_layers = 500
   min_layer_vol = 0.025
   min_layer_thick = 0.15
   max_layer_thick = 1.50
   density_model = 1
/
!-------------------------------------------------------------------------------
! mixing
!-------------------------------------------------------------------------------
&mixing
  surface_mixing = 1
  coef_mix_conv  = 0.2
  coef_wind_stir = 0.23
  coef_mix_shear = 0.3
  coef_mix_turb  = 0.51
  coef_mix_KH    = 0.3
  deep_mixing    = 2
  coef_mix_hyp   = 0.5
  diff = 0.
/
!--------------------------------------------------------------------------------
&wq_setup
!!-- Choose 'aed2' or 'fabm' (aed2 is the default)
    wq_lib = 'aed2'
    wq_nml_file = 'aed2.nml'
    ode_method = 1
    split_factor = 1
    bioshade_feedback = .true.
    repair_state = .true.
    mobility_off = .false.
!   multi_ben = .true.
!   benthic_mode = 1
/
!/
!-------------------------------------------------------------------------------
! morphometry
!-------------------------------------------------------------------------------
&morphometry
   lake_name  = 'FCR'
   latitude   =     37
   longitude  =    -80
   crest_elev =    312
   bsn_len    =    670
   bsn_wid    =    450
   bsn_vals   =     34
   !   H(m) & A(m2), both have length bsn_vals
   H = 302.1,   302.4,   302.7,   303.0ï¼Œ  303.3,   303.6,   303.9,   304.2, 
           304.5,   304.8,   305.1,   305.4,   305.7,   306.0,   306.3,
           306.6,   306.9,   307.2,   307.5,   307.8,   308.1,   308.4, 
           308.7,   309.0,   309.3,   309.6,   309.9,   310.2,   310.5, 
           310.8,   311.1,   311.4,   311.7,   312.0,   312.3
   A = 0,    623,  1009,         1454,       1982,     2577,       5655,       6247,
            6879,  7542,         8249,       8982,      9754,      13981,      15100,
           16252,   17427,      18617,      19809,     28682,      30705,      32779,   
